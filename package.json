{
  "name": "chrome-launcher-cli",
  "version": "0.1.0",
  "description": "Chrome Launcher for CLI",
  "license": "MIT",
  "repository": "ragingwind/chrome-launcher-cli",
  "author": {
    "name": "ragingwind",
    "email": "ragingwind@gmail.com",
    "url": "ragingwind.me"
  },
  "bin": {
    "chrome": "./dist/cli.js"
  },
  "engines": {
    "node": ">=4"
  },
  "scripts": {
