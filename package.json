  "devDependencies": {
    "@types/core-js": "^0.9.41",
    "@types/node": "^8.0.2",
    "chrome-launcher": "^0.2.1",
    "xo": "^0.18.2"
  },
  "xo": {
    "semicolon": false,
    "env": {
      "browser": true
    },
    "globals": [
      "window"
    ]
